		<identity impersonate="true"/>
		<customErrors mode="Off"/>
		<!-- Fle upload - set execution timeout and maxrequest length - currently 10 MB-->
		<httpRuntime executionTimeout="200" maxRequestLength="11264" requestLengthDiskThreshold="80" useFullyQualifiedRedirectUrl="false" minFreeThreads="8" minLocalRequestFreeThreads="4" appRequestQueueLimit="5000" enableKernelOutputCache="true" enableVersionHeader="true" requireRootedSaveAsPath="true" enable="true" shutdownTimeout="90" delayNotificationTimeout="5" waitChangeNotification="0" maxWaitChangeNotification="0" enableHeaderChecking="true" sendCacheControlHeader="true" apartmentThreading="false"/>
